            <h1>Dashboard</h1>
        </div>
    </div>

    <div class="row">
        <div class="col-md-6">
            <h2>Recent Orders</h2>
            <table class="table table-striped table-bordered">
                <thead>
                <tr>
                    <th>Name</th>
                    <th>Amount</th>
                </tr>
                </thead>
            </table>
        </div>
        <div class="col-md-6">
            <h2>Other Table</h2>
            <table class="table table-striped table-bordered">
                <thead>
                <tr>
                    <th>Test</th>

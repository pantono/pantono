{
    "name": "csburton/pantona",
    "description": "Simple Ecommerce Framework built on Silex",
    "require": {
        "silex/silex": "~1.2",
        "twig/twig": "1.12",
        "dflydev/doctrine-orm-service-provider": "~1.0.6",
        "symfony/yaml": "~2.6",
        "symfony/form": "~2.6",
        "symfony/translation": "~2.6",
        "symfony/validator": "~2.6",
        "symfony/config": "~2.6",
        "symfony/security-csrf": "~2.6",
        "symfony/security" : "~2.6",
        "symfony/twig-bridge": "~2.6",
        "predis/predis" : "1.0.x",
        "phpmd/phpmd": "@stable"
    },
    "require-dev" : {
      "phpunit/phpunit" : "4.5.x"
    },
    "authors": [
        {
            "name": "Chris Burton",
            "email": "csburton@gmail.com"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "Pantona\\" : "src/Pantona"
        }
    },
    "config": {
        "bin-dir" : "bin"
    }
}
